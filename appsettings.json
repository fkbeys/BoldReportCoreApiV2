{
  "Serilog": {
    "MinimumLevel": "Error",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs\\AppLogs.log"
        }
      }
    ]
  },

  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Warning"
  //  }
  //},
  "AllowedHosts": "*",
  "appSettings": {
    "ExtAssemblies": "BoldReports.Data.WebData;BoldReports.Data.Csv;BoldReports.Data.Excel;",
    "IsOfflineLicense": "false"
  }
}
